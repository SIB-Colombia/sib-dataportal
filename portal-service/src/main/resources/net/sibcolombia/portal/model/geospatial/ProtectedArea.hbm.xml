<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="protected_area" name="net.sibcolombia.portal.model.geospatial.ProtectedArea">
    <id unsaved-value="null" name="paId" column="pa_id">
      <generator class="assigned"/>
    </id>
    <property name="protectedAreaId" column="id"/>
    <property name="name" column="name"/>
    <property name="description" column="pn_cat"/>
    <property name="occurrenceCount" column="occurrence_count"/>
    <property name="speciesCount" column="species_count"/>
  </class>
</hibernate-mapping>
