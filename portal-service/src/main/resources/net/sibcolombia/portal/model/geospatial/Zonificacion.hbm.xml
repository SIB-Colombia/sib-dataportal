<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="zonificacion" name="net.sibcolombia.portal.model.geospatial.Zonificacion">
    <id unsaved-value="null" name="szh" column="szh">
      <generator class="assigned"/>
    </id>
    <property name="zonificacionId" column="id"/>
    <property name="nomszh" column="nomszh"/>
    <property name="occurrenceCount" column="occurrence_count"/>
    <property name="speciesCount" column="species_count"/>
  </class>
</hibernate-mapping>