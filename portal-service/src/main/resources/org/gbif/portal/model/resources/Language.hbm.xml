<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="language" name="org.gbif.portal.model.resources.Language">
     <id unsaved-value="null" name="isoLanguageCode" column="iso_language_code">
      <generator class="assigned"/>
    </id>
    <property name="languageId" column="id"/>
    
    <property name="name" column="name" not-null="false"/>
    
    <property name="standard" column="standard" not-null="false"/>
    
  </class>
</hibernate-mapping>